    Customer <|-- Cash
    Customer <|-- Credit
    Customer "1"--o"*" Barang : 
    
    class Customer{
      <<abstract>>
      #int IDcustomer
      -String nama
      -String alamat
      +int nextIDcustomer()
    }
    
    class Cash{
      -String tgl_pembayaran
    }
    class Credit{
      -String contact
    }
    class Barang{
      -String jenisBuku
      -double hargaBuku
      +String getJenisBuku()
      +double getHargaBuku()
    }
            
